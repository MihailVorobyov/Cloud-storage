<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="160.0"
      prefWidth="270.0"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.vorobyov.cloudstorage.client.sample.InputNameDialogController">
   <Button fx:id="cancelButton"
           layoutX="15.0"
           layoutY="111.0"
           mnemonicParsing="false"
           onAction="#cancel"
           text="Cancel">
      <font>
         <Font size="14.0"/>
      </font>
   </Button>
   <Button fx:id="okButton"
           layoutX="189.0"
           layoutY="111.0"
           mnemonicParsing="false"
           onAction="#ok"
           prefHeight="30.0"
           prefWidth="61.0"
           text="OK">
      <font>
         <Font size="14.0"/>
      </font>
   </Button>
   <Label fx:id="messageLabel"
          layoutX="16.0"
          layoutY="22.0"
          prefHeight="20.0"
          prefWidth="238.0">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <TextField fx:id="nameField"
              layoutX="16.0"
              layoutY="61.0"
              prefHeight="25.0"
              prefWidth="238.0"
              promptText="Enter new file name"/>
</Pane>
